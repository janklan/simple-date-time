{
    "name": "janklan/simple-date-time",
    "description": "Timezone-agnostic Date and Time value objects for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Klan",
            "email": "jan@klan.com.au"
        }
    ],
    "keywords": [
        "date",
        "time",
        "datetime",
        "timezone-agnostic",
        "value-object",
        "immutable"
    ],
    "require": {
        "php": "~8.2"
    },
    "require-dev": {
        "brianium/paratest": "^7.15",
        "ergebnis/composer-normalize": "^2.48",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpdoc-parser": "^2.3",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "~12.0",
        "rector/rector": "^2.2",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Janklan\\SimpleDateTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Janklan\\SimpleDateTime\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "minimum-stability": "stable"
}
